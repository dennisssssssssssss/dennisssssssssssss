5.int a[10][10],m,n;
    cin>>m>>n;
    for(int i=1;i<=m;i++)
        for(int j=1;j<=n;j++)
        if(i==1)
        a[i][j]=j;
    else if(j==1)
        a[i][j]=i;
    else a[i][j]=a[i][j-1]+a[i-1][j];
    for(int i=1;i<=m;i++, cout<<endl)
        for(int j=1;j<=n;j++)
            cout<<a[i][j]<<" ";
