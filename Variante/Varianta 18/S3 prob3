#include <iostream>
using namespace std;

int count(float a[100], int n){
	
	int i;
	float s=0,medie;
		
	for (i = 0; i < n; i++)
		suma=suma + a[i];

	medie=s/n;
	
	int nr=0;
		
	for (i = 0; i < n; i++)
		if (a[i] >= medie)
			nr++;

	return nr;

}

int main(){
	
	int n;
	cin>>n;

	int i;
	float a[100];
	for (i = 0; i < n; i++)
		cin>>a[i];

	cout<<count(a,n);
	return 0;
}
